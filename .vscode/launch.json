{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Premiere",
            "port": 7777,
            "webRoot": "${workspaceFolder}/src/index.html",
            "preLaunchTask": "gulp: run",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "*": "${workspaceRoot}/*"
            }
        }
    ]
}